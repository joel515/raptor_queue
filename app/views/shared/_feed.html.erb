<% if @feed_items.any? %>
  <table class="table">
    <thead>
      <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Status</th>
        <th>Solver</th>
        <th>Nodes</th>
        <th>PPN</th>
        <th></th>
      </tr>
    </thead>
    <%= render @feed_items, cols: { id: true, name: true, status: true,
                                    config: true, nodes: true,
                                    processors: true, buttons: true } %>
  </table>
  <%= paginate @feed_items %>
<% end %>
