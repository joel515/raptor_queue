<% config = @job.config if config.nil? %>
<% if config.nil? %>
  <%= form.select :version, {}, { prompt: "Select version" },
    { class: "#{control}", disabled: true } %>
<% else %>
  <%= form.select :version, eval("#{@job.config.capitalize}Job::VERSIONS.map " \
    "{ |k,v| [v, k.to_s] }"), { prompt: "Select version" },
    { class: "#{control}" }%>
<% end %>
